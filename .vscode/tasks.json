{
    "version": "2.0.0",
    "command": "cmd",
    "echoCommand": true,
    "args": [
        "/C"
    ],
    "tasks": [
        {
            "label": "system_beeps_bbc.ssd",
            "type": "shell",
            "args": [
                "\".\\system_beeps_bbc\\make.bat\""
            ],
            "problemMatcher": {
                "owner": "6502",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ],
                "pattern": {
                    "regexp": "^(.*):(\\d+):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4
                }
            },
            "group": "build"
        },{
